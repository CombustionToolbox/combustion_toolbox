N = length(app.PS.strP);
for j=N:-1:1
    T_guess(j) = app.PS.strP{1,j}.T_guess;
    p_guess(j) = app.PS.strP{1,j}.p_guess * 1e-5;
    T(j) = app.PS.strP{1,j}.T;
    p(j) = app.PS.strP{1,j}.p;
end

figure
hold on
ax = axes(gca);
set(ax,'LineWidth',config.linewidth,'FontSize',config.fontsize-2,'BoxStyle','full')
xlabel(ax, 'Equivalence ratio','FontSize',config.fontsize,'interpreter','latex');
ylabel(ax, 'Temperature [K]','FontSize',config.fontsize,'interpreter','latex');
plot(app.PD.phi.value, T, 'linewidth', 1.5)
plot(app.PD.phi.value, T_guess, 'linewidth', 1.5)
plot(app.PD.phi.value, 6*app.PS.strR{1,j}.T, 'linewidth', 1.5)

figure
hold on
ax = axes(gca);
set(ax,'LineWidth',config.linewidth,'FontSize',config.fontsize-2,'BoxStyle','full')
xlabel(ax, 'Equivalence ratio','FontSize',config.fontsize,'interpreter','latex');
ylabel(ax, 'Pressure [bar]','FontSize',config.fontsize,'interpreter','latex');
plot(app.PD.phi.value, p, 'linewidth', 1.5)
plot(app.PD.phi.value, p_guess, 'linewidth', 1.5)
plot(app.PD.phi.value, 15*app.PS.strR{1,j}.T, 'linewidth', 1.5)